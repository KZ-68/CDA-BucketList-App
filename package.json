    "test:a11y": "dotenv -e .env.test -- jest -i __tests__/a11y",
    "prisma-db-push:test": "dotenv -e .env.test -- npx prisma db push",
    "prisma-db-push:dev": "dotenv -e .env.development -- npx prisma db push",
    "prisma-studio:test": "dotenv -e .env.test -- npx prisma studio",
    "prisma-studio:dev": "dotenv -e .env.development -- npx prisma studio"
  },
  "prisma": {
    "seed": "node prisma/seed.js",
    "schema": "./schema"
  },
  "type": "module",
  "dependencies": {
